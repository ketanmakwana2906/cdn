var captchaUnable = false
        try {
             result = await axios(`https://www.confirmtkt.com/pnr-status/${pnr}?`)
             myCache.del("ip_list");
            
		
	    proxy_ips = myCache.get( "ip_list" );
            if (proxy_ips==undefined){
                logger.log({
                    level: 'info',
                       message: `Finding IP List from Oxylabs url using creds.`,
                        label: "OxyLabs"
                });
                proxy_ips = await getOxyLabsIps();
                proxy_ips = proxy_ips.split("\n")
                success = myCache.set( "ip_list", proxy_ips, 3600 );
            }
            random_proxy_ip = proxy_ips[Math.floor(Math.random()*proxy_ips.length)];
            ip_data = random_proxy_ip.split(":");
            domain = ip_data[0];
            port = ip_data[1];
            user_agent = randomUseragent.getRandom();
            logger.log({
                level: 'info',
                message: `Proxy IP hit by OxyLabs: ${ip_data}`,
                label: "OxyLabs"
            });
            result = await axios({
                method:'get',
                url: `https://www.confirmtkt.com/pnr-status/${pnr}?`,
                headers: {
                    'User-Agent': String(user_agent)
                },
                proxy:{
                    protocol: 'http',
                    host: String(domain),
                    port: port,
                    auth: {
                        username: config.OxyLabsCreds.username,
                        password: config.OxyLabsCreds.password
                    }
                }
            })

            htmlData = result.data.replace(/(\r)/gm,"")
            return {'html':htmlData}
