async function scrapConfirmTkt(pnr) {
    const args = [
        '--no-sandbox',
        '--disable-setuid-sandbox',
        '--disable-dev-shm-usage',
        '--disable-accelerated-2d-canvas',
        '--disable-gpu',
        '--window-size=1920x1080'
    ];

    const options = {
        args,
        userAgent: randomUA.generate(),
        headless: true,
        ignoreHTTPSErrors: true,
        defaultViewport: null,
        ignoreDefaultArgs: ["--enable-automation"]
    };

    try {   
        const browser = await puppeteer.launch(options);
        const page = await browser.newPage();

        logger.log({
            level: 'info',
            message: `Page Loading ${pnr}`,
            label: 'Indian Rail'
        });

        page.on('error', err => {
            browser.close();
        });

        var response = '';
        try {
            // await page.waitFor(500);
            await page.goto('https://www.confirmtkt.com/pnr-status');

            await page.addScriptTag({ url: 'https://code.jquery.com/jquery-3.2.1.min.js' });
            await page.addScriptTag({ url: 'https://www.google.com/recaptcha/api.js?render=6LdT9ogUAAAAACf7wlZ5dsVfsDj-21XnSTwVPSvo' });

            try {
                logger.log({
                    level: 'info',
                    message: `Page loaded ${pnr}`,
                    label: 'Indian Rail'
                });

                

                await page.evaluate((PNR) => { $('#pnrtext').val(PNR) }, pnr);
                
                await Promise.all([
                    // Manual clicking of the link
                    page.$eval('form button', el => el.click()),
                    page.waitForNavigation()

                ]).catch(e => console.log(e));

                await page.waitFor(2000)
                logger.log({
                    level: 'info',
                    message: `Navigate on pnr page`,
                    label: 'Indian Rail'
                });

                response = await page.evaluate(() => {
                    
                    return new Promise((resolve) => {
                        domSpan =  document.body.innerHTML//document.getElementsByTagName('html')[0]
                        pnrQuerySelector = document.querySelectorAll('#pnrFullDetails .main>table.table span')
                        pnrStatus = []

                        resolve({'html':domSpan})
                    })


                })

            } catch (e) {
                if (e instanceof TimeoutError) {
                    logger.log({
                        level: 'error',
                        message: `Error Occurred in getting data ${e}`,
                        label: 'Indian Rail'
                    });
                }
            }

        } catch (e) {
            if (e instanceof TimeoutError) {
                logger.log({
                    level: 'error',
                    message: `Error Occurred in loading page ${e}`,
                    label: 'Indian Rail'
                });
            }

        }
        finally {
            if (!page.isClosed()) {
                await page.close();
            }
            await browser.close();
        }
        logger.log({
            level: 'info',
            message: `pnr Scrapped ${pnr}`,
            label: 'Indian Rail'
        });
        return response;
    } catch (e) {
        logger.log({
            level: 'error',
            message: `Error Occurred in Launching page ${e}`,
            label: 'Indian Rail'
        });

    }


}


issue in above is when page loaded one loader comes and immediately html of that while loader is returned
like below , but want to catch html content after of this loader 
