import pandas as pd

# Load the Excel file
file_path = "coupons.xlsx"  # Replace with your Excel file name
data = pd.read_excel(file_path)

# Extract the coupon codes from the first column
coupon_codes = data.iloc[:, 0].tolist()

# SQL INSERT statement generation
output_file = "insert_queries.sql"
with open(output_file, "w") as f:
    f.write("INSERT INTO marketing_zomato_gold_coupons (coupon_code, created_at, is_used, tm_booking_id, updated_at, user_id) VALUES\n")
    for i, coupon in enumerate(coupon_codes):
        query_part = f'("{coupon}", NOW(), FALSE, NULL, NULL, NULL)'
        if i < len(coupon_codes) - 1:
            query_part += ",\n"
        else:
            query_part += ";\n"
        f.write(query_part)

print(f"SQL queries written to {output_file}")
